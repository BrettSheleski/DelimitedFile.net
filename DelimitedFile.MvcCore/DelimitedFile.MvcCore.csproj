<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'==''">net47;net48;netstandard2.0;netstandard2.1;net5;net6;net7</TargetFrameworks>
    <RootNamespace>Sheleski.DelimitedFile.MvcCore</RootNamespace>
    <AssemblyName>Sheleski.DelimitedFile.MvcCore</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0.6</Version>
    <Authors>Brett Sheleski</Authors>
    <Company>Brett Sheleski</Company>
    <Description>Helpers for Sheleski.DelimitedFile for ASP.NET MVCCore.</Description>
    <Copyright>Copyright Â© Brett Sheleski 2021</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/BrettSheleski/DelimitedFile.net</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/BrettSheleski/DelimitedFile.net</RepositoryUrl>
    <PackageTags>CSV, Tab Delimited, Delimited File, MVC</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Abstractions" Version="2.2.0" />
    <PackageReference Include="Sheleski.DelimitedFile" Version="0.1.0.6" />
  </ItemGroup>

</Project>
